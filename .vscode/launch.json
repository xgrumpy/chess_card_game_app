    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch Chewker",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Server/bin/Debug/net7.0/GiraffeExample.App.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Server",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Chewker Client via Yarn",
            "runtimeExecutable": "yarn",
            "cwd": "${workspaceFolder}/vite_project",
            "runtimeArgs": ["dev"],
            "port": 5173
        }
    ]
}